{
  "name": "apolis-app",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest",
    "lint": "eslint .",
    "preview": "vite preview",
    "start:prod": "vite dev --mode production",
    "start:staging": "vite dev --mode staging",
    "build:prod": "vite build --mode production && aws s3 --profile apolis sync --delete ./dist s3://apolis-web && aws --profile apolis cloudfront create-invalidation --distribution-id E261GIL5FUBKTG --paths \"/*\" > /dev/null",
    "build:staging": "vite build --mode staging && aws s3 --profile apolis sync --delete ./dist s3://apolis-web-staging && aws --profile apolis cloudfront create-invalidation --distribution-id E188H7YFAZPPIV --paths \"/*\" > /dev/null"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@tanstack/react-query": "^5.65.1",
    "@tanstack/react-router": "^1.97.18",
    "aws-amplify": "^6.12.1",
    "bootstrap": "^5.3.3",
    "lucide-react": "^0.474.0",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.8",
    "react-dom": "^18.3.1",
    "react-select": "^5.9.0",
    "recharts": "^2.15.0",
    "sass": "^1.83.4",
    "use-debounce": "^10.0.4",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@tanstack/eslint-plugin-query": "^5.65.0",
    "@tanstack/react-query-devtools": "^5.65.1",
    "@tanstack/router-devtools": "^1.97.18",
    "@tanstack/router-plugin": "^1.97.18",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "vite": "^6.0.5",
    "vitest": "^3.0.6"
  }
}
